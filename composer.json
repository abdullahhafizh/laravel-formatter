{
    "name": "abdullahhafizh/laravel-formatter",
    "type": "library",
    "description": "A formatting library that converts data output between XML, CSV, JSON, TXT, YAML and a few others.",
    "keywords": ["laravel", "formatter", "data", "convert", "csv", "xml", "yaml"],
    "homepage": "http://github.com/abdllhhafizh/laravel-formatter",
    "version": "1.0.0",
    "require": {
        "php": "^7.2",
        "league/csv": "~9.0",
        "mustangostang/spyc": "~0.6",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "Hafizh\\Formatter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Formatter": "Hafizh\\Formatter"
            }
        }
    },
    "minimum-stability": "stable"
}
